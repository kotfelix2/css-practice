<footer class="row footer g-0">
  {{#with footer}}
    <div class='nav col-9 {{#if isVertical}}nav_vertical{{else}}nav_inline{{/if}}'>
      {{#each pages}}
        <a href='{{path}}' class='nav_link'>{{name}} </a>
      {{/each}}
    </div>

    <div class='col-3'>
        <span class="footer_copyright">{{copyright}}</span>
    </div>
  {{/with}}
</footer>